<script>
$(function(){
    $('.predict-s,.predict-s-line').mouseover(function(){
        $(this).addClass('predictor_notify');
        $(this).children("div[id!='hint-box']").removeClass('predictor_count_hide').addClass('predictor_count_show');
    }).mouseout(function(){
        $(this).removeClass('predictor_notify');
        $(this).children("div[id!='hint-box']").removeClass('predictor_count_show').addClass('predictor_count_hide');
    });
});
</script>


<link rel=stylesheet href="/build/css/predictgame/green_hint-f603a47f17.css?v=20210809">
<script src="includes/js/jquery_cookie.js"></script>
<script>
$(function(){
    var $firstGreen = $('table.predictgame-table tr td.predict-s-win').eq(0); //找到第一個出現綠色文字的地方
    if ($firstGreen.length == 1) {
        var greenHintContent =
        '<div id="hint-box"><div><a class="closebox">close</a><strong>綠色欄位</strong>表示過盤<span></span></div></div>';
        $(greenHintContent).appendTo($firstGreen);
        $firstGreen.css("position", "relative");
        $firstGreen.children().css("text-align", "center");
        $(function(){
            $('table.predictgame-table div#hint-box').click(function(){
                $.get('predictgame.php?action=closeGreenHint');
                $(this).hide();
                $.cookie("stop_green_hint_scale", true, {expires: 365})
            });
        });

        //出現過警示，cookie值+1
        var greenCookieCount = $.cookie("green_hint_scale");
        greenCookieCount = (greenCookieCount == null) ? 1 : parseInt(greenCookieCount) + 1;
        $.cookie("green_hint_scale", greenCookieCount, {expires: 365})
    }
});
</script>

